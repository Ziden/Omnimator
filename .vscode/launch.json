{
    "version": "0.2.0",
    "configurations": [
      
      {
      "type": "node",
      "request": "launch",
      "name": "Launch Jest",
      "program": "${workspaceRoot}/node_modules/jest/bin/jest.js",
      "cwd": "${workspaceRoot}",
      "args": ["--runInBand", "testNamePatternA=Angle"],
      "runtimeArgs": [
        "--nolazy"
      ],
      "env": {
        "NODE_ENV": "development"
      },
      "sourceMaps": true
    }]
  }